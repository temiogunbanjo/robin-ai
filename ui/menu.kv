## garden install kivy icons
<Menu>:
    name: "settings"
    orientation: "vertical"
    padding: (0, 0)
    spacing: "10dp"

    # black background
    canvas.before:
        Color:
            rgba: 23/255, 23/255, 23/255, 1
        Rectangle:
            size: self.size

<MenuNavigationBar>:
    orientation: "horizontal"
    height: "55dp"
    size_hint_y: None

    canvas.before:
        Color:
            rgba: 76/255, 175/255, 80/255, 1
        Rectangle:
            pos: self.pos
            size: self.size

    StackLayout:
        size_hint: 0.4, 1
        orientation: 'lr-tb'
        spacing: "15dp"
        Button:
            background_color: (10, 10, 10, 0.08)
            padding: "20dp", "0"
            text: "<"
            font_name: "Poppins"
            size: self.texture_size
            size_hint: None, 1
            on_release:
                app.root.transition.direction = 'right'
                app.root.current = "home"

<MenuBody>:
    orientation: "vertical"

    Label:
        text: "Element 1"
        size: self.texture_size
        halign: "left"

    Label:
        text: "Element 2"

    Label:
        text: "Element 3"

    Label:
        text: "Element 4"